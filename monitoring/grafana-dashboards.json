{
  "dashboard": {
    "id": null,
    "title": "Octabyte Monitoring Dashboard",
    "panels": [
      {
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "node_cpu_seconds_total",
            "legendFormat": "CPU"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Request Latency",
        "targets": [
          {
            "expr": "http_request_duration_seconds",
            "legendFormat": "Latency"
          }
        ]
      }
    ]
  },
  "overwrite": true
}
